{
  "name": "eBay Native Link Generator",
  "version": "0.0.1",
  "description": "Creates Native Links for eBay Application",
  "manifest_version": 2,
  "homepage_url": "https://ebay.com",
  "icons": {
    "32": "deeplink-logo-32x32.png",
    "128": "deeplink-logo-144x144.png"
  },
  "browser_action": {
    "default_icon": "deeplink-logo-32x32.png",
    "default_title": "eBay Native Link Generator",
    "default_popup": "index.html"
  },
  "permissions": ["tabs", "activeTab", "https://*/", "management"],
  "content_security_policy": "script-src 'self' 'sha256-0wCySo4xovAdrardRZQ2Lg88jGxjalFxUdpxVH5GUpA='; object-src 'self'"
}
