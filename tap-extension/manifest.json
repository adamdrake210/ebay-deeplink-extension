{
  "name": "eBay App Deeplink Generator ",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Creates deeplink/nativelinks for the eBay App",
  "homepage_url": "https://ebay.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "TAP",
    "default_popup": "src/browser_action/tap.html"
  }
}
